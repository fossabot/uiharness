{
  "name": "example.electron",
  "version": "0.0.0",
  "description": "Example usage of `@uiharness/electron`",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "postinstall__": "uiharness-electron init",
    "start": "node .",
    "test": "sh ./node_modules/@tdb/test/sh/test.sh",
    "lint": "sh ./node_modules/@tdb/typescript/sh/lint.sh $@",
    "build": "sh ./node_modules/@tdb/typescript/sh/build.sh $@",
    "prepare": "yarn run build && yarn run lint && yarn test"
  },
  "dependencies": {
    "@tdb/util": "1.9.22"
  },
  "devDependencies": {
    "@tdb/test": "1.2.10",
    "@tdb/typescript": "3.2.5"
  },
  "license": "MIT",
  "private": true
}
