{
  "name": "example.electron",
  "version": "0.0.57",
  "description": "Example usage of `@uiharness/electron`",
  "scripts": {
    "postinstall": "uiharness-electron init",
    "ui": "yarn sl && uiharness-electron $@",
    "sl": "msync sl && sh ./sh/copy-bin.sh",
    "reset": "yarn sl && uiharness-electron init --reset",
    "nuke": "sh ./sh/nuke.sh"
  },
  "dependencies": {
    "@uiharness/core": "^1.0.64",
    "@uiharness/electron.ui": "0.0.36"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@uiharness/electron": "0.3.21"
  },
  "resolutions": {
    "@types/react": "16.7.17"
  },
  "license": "MIT",
  "private": true
}
