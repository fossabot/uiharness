{
  "name": "electron",
  "version": "0.0.0",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "postinstall": "uiharness init",
    "chmod": "sh ./sh/chmod.sh",
    "pre": "yarn chmod",
    "lint": "sh ./node_modules/@tdb/typescript/sh/lint.sh $@",
    "build": "sh ./node_modules/@tdb/typescript/sh/build.sh $@",
    "prepare": "yarn run lint && yarn run build",
    "start": "yarn pre && uiharness start",
    "bundle": "yarn pre && uiharness bundle",
    "stats": "yarn pre && uiharness stats",
    "serve": "serve -s dist"
  },
  "dependencies": {},
  "devDependencies": {
    "@uiharness/parcel": "^1.0.23"
  },
  "license": "MIT"
}
