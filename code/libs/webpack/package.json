{
  "name": "@uiharness/webpack",
  "description": "React/webpack builder for UIHarness.",
  "version": "1.0.2",
  "main": "./lib/index.js",
  "scripts": {
    "build": "sh ./node_modules/@tdb/typescript/sh/build.sh $@",
    "lint": "sh ./node_modules/@tdb/typescript/sh/lint.sh $@",
    "prepare": "yarn build && yarn lint"
  },
  "bin": {
    "uiharness": "./lib/bin/uiharness.js"
  },
  "dependencies": {
    "@rescripts/cli": "0.0.7",
    "@rescripts/rescript-env": "0.0.3",
    "@tdb/log": "1.1.17",
    "@tdb/test": "1.2.10",
    "@tdb/typescript": "3.2.5",
    "@tdb/util": "1.9.22",
    "fs-extra": "7.0.1",
    "js-yaml": "3.12.1",
    "raw-loader": "1.0.0",
    "react-scripts": "2.1.3",
    "serve": "10.1.1",
    "shelljs": "0.8.3"
  },
  "devDependencies": {
    "@types/fs-extra": "5.0.4",
    "@types/js-yaml": "3.11.4",
    "@types/shelljs": "0.8.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "license": "MIT",
  "repository": "github.com/uiharness/uiharness"
}
